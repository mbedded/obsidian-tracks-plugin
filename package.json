    "builtin-modules": "5.0.0",
    "esbuild": "0.25.9",
    "esbuild-svelte": "^0.9.3",
    "obsidian": "latest",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "2.8.1",
    "typescript": "5.9.2"
  }
